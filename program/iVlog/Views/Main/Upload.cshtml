<script type="text/javascript">
    function video_previewFile() {
        var preview = document.querySelector('video[id=video-upload]');
        var file = document.querySelector('input[id=video-upload_input]').files[0];
        var reader = new FileReader();
        reader.onloadend = function () {
            preview.src = reader.result;
            console.log("上传完成");
            document.getElementById("upload-state").innerHTML = "上传成功";
            document.getElementById("progress_line").style.setProperty("background-color", "#00ACE5");
        }
        if (file) {
            reader.readAsDataURL(file);
        } else {
            preview.src = "";
        }
